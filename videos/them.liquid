  #OHT-menudelay{
    animation: menudelay 1s ease 0s;
    transform-origin: bottom;
    transition-delay: 0s;
  }
    
  @keyframes menudelay {
  from {
    transform: scaleY(0);
    
    opacity:0;
  }

  to {
    transform: scaleY(1);
    
    opacity:1;
  }
  }
    
   #OHT-elsedelay{
    animation: elsedelay 1.5s ease 0s;
    transform-origin: left;
  }
    
  @keyframes elsedelay {
  from {
    transform: translateY(100px);
    opacity:0;
  }

  to {
    transform: translateY(0px);
    opacity:1;
  }
  }
  
